<MudCard Elevation="1">
    <MudCardHeader>
        <CardHeaderAvatar>
            <MudAvatar Image="images/question.png" Size="Size.Small" Square="true"/>
        </CardHeaderAvatar>
    </MudCardHeader>
    <MudCardContent>
        <MudText Typo="Typo.body1">@Question</MudText>
    </MudCardContent>
    <MudCardActions Style="justify-content:right">
        <MudIconButton Icon="@Icons.Material.Filled.QuestionAnswer"/>
    </MudCardActions>
</MudCard>

@code {
    [Parameter] public string? Question { get; set; }

}