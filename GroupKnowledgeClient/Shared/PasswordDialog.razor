<MudDialog>
    <DialogContent>
        <MudTextField Label="Password" @bind-Value="AgentPassword" Variant="Variant.Outlined" InputType="InputType.Password"/>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Primary" @onclick=@Submit>Ok</MudButton>
    </DialogActions>
</MudDialog>

@code {
    private string AgentPassword { get; set; } = string.Empty;

    [CascadingParameter] 
    public MudDialogInstance? MudDialog { get; set; }

    private void Submit()
    {
        MudDialog?.Close(DialogResult.Ok(AgentPassword));
    }

    private void Cancel() => MudDialog?.Cancel();
}